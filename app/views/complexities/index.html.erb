<p id="notice"><%= notice %></p>

<h1>Listing Complexities</h1>

<table  class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @complexities.each do |complexity| %>
      <tr>
        <td><%= complexity.name %></td>
        <td><%= link_to 'Show', complexity, class: "btn btn-xs  btn-primary"  %></td>
        <td><%= link_to 'Edit', edit_complexity_path(complexity), class: "btn btn-xs  btn-primary"  %></td>
        <td><%= link_to 'Destroy', complexity, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-xs  btn-danger"  %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Complexity', new_complexity_path %>
