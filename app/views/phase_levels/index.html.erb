<p id="notice"><%= notice %></p>

<h1>Listing Phase Levels</h1>

<table  class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phase_levels.each do |phase_level| %>
      <tr>
        <td><%= phase_level.name %></td>
        <td><%= link_to 'Show', phase_level, class: "btn btn-xs  btn-primary" %></td>
        <td><%= link_to 'Edit', edit_phase_level_path(phase_level), class: "btn btn-xs  btn-primary" %></td>
        <td><%= link_to 'Destroy', phase_level, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-xs  btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phase level', new_phase_level_path %>
